@{
    ViewData["Title"] = "UploadaFiles";
}
<hr />
<div class="jumbotron jumbotron-fluid">
    <div class="container">
        @if (ViewData["Error"] != null)
        {
            <h4>@ViewData["Error"]</h4>
        }
    </div>
    <div class="container">
        @if (ViewData["Result"] != null)
        {
            <h4>@ViewData["Result"]</h4>
            <hr />
            @if (ViewBag.Archives != null)
            {
                @foreach (string arquivo in ViewBag.Archives)
                {
                    <li>
                        @arquivo
                    </li>
                }
            }
        }
    </div>
</div>
<div class="form-group">
    <a class="btn btn-outline-info" asp-controller="AdminImages" asp-action="GetImagens">Exibir Imagens</a>
</div>
